generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource codeForge {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id
  email     String  @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}